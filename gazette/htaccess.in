# Make sure you have AllowOverride AuthConfig set for this directory... 
AuthType Basic
AuthName "Ghillie Gazette"
# (Following line optional)
AuthBasicProvider file
AuthUserFile %DEST%/.users

<Files "*2014*">
	Require valid-user
</Files>

<Files "*2013-11*">
	Require valid-user
</Files>

<Files "*2013-09*">
	Require valid-user
</Files>

<Files "directory.pdf">
	Require valid-user
</Files>

<Files ".users">
	Deny from all
</Files>
